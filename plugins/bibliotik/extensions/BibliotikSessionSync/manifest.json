{
  "manifest_version": 2,
  "name": "Bibliotik Session Sync",
  "description": "This extension provides Bibliotik session synchronization for Harvest.",
  "version": "0.0.1",
  "permissions": [
    "storage",
    "cookies",
    "*://*.bibliotik.me/"
  ],
  "background": {
    "scripts": [
      "common.js",
      "background.js"
    ]
  },
  "options_ui": {
    "page": "settings.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.bibliotik.me/*"
      ],
      "js": [
        "common.js",
        "content.js"
      ]
    }
  ]
}
